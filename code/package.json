{
  "name": "ask-a-question-rubber-duck",
  "displayName": "Ask a question Rubber Duck",
  "description": "Rubber ducking is a powerful method for solving even the most complex problems",
  "version": "0.0.1",
  "publisher": "brunolm",
  "repository": {
    "type": "git",
    "url": "git@github.com:agencyenterprise/rubber-duck.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.rubberDuck"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.rubberDuck",
        "title": "Rubber Duck: Ask a question"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
